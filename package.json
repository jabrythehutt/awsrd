{
  "name": "awsrd",
  "version": "0.0.0-PLACEHOLDER",
  "displayName": "AWS Remote Development",
  "description": "Provision and connect to your AWS EC2 Instances with VS Code",
  "type": "module",
  "main": "./extension.cjs",
  "publisher": "jabrythehutt",
  "categories": [
    "Other"
  ],
  "icon": "media/ec2.png",
  "keywords": [
    "AWS",
    "EC2",
    "Remote SSH",
    "SSM"
  ],
  "repository": {
    "type": "github",
    "url": "https://github.com/jabrythehutt/awsrd"
  },
  "author": {
    "name": "Daniel Jabry",
    "email": "djabry@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-ec2": "3.670.0",
    "@aws-sdk/client-ssm": "3.670.0",
    "@aws-sdk/types": "3.664.0",
    "@smithy/smithy-client": "3.4.0",
    "@smithy/util-stream": "3.1.9",
    "@aws-sdk/client-sts": "3.670.0",
    "@aws-sdk/region-config-resolver": "3.664.0",
    "@aws-sdk/client-ec2-instance-connect": "3.670.0",
    "@aws-sdk/credential-providers": "3.670.0",
    "@smithy/shared-ini-file-loader": "3.1.8",
    "aws-cdk-lib": "2.162.1",
    "cdk-monitoring-constructs": "8.3.1",
    "constructs": "10.4.2",
    "lodash": "4.17.21",
    "sshpk": "1.17.0",
    "yargs": "17.7.1",
    "tslib": "2.5.0",
    "rxjs": "7.8.0",
    "validator": "13.9.0"
  },
  "devDependencies": {
    "aws-cdk": "2.162.1",
    "@types/aws-lambda": "8.10.145",
    "@types/validator": "13.7.14",
    "@types/lodash": "4.14.192",
    "@types/vscode": "1.76.0",
    "@types/node": "^18.15.5",
    "@types/sshpk": "^1.17.1",
    "@types/yargs": "^17.0.23",
    "@types/jest": "29.5.0",
    "typescript": "4.9.5",
    "ttypescript": "1.5.15",
    "ts-transformer-keys": "0.4.4",
    "jest": "29.5.0",
    "jest-cli": "29.5.0",
    "jest-junit": "16.0.0",
    "@vscode/vsce": "3.1.1"
  },
  "extensionDependencies": [
    "ms-vscode-remote.remote-ssh"
  ],
  "engines": {
    "vscode": "^1.76.0"
  },
  "badges": [
    {
      "description": "Maintainability",
      "href": "https://codeclimate.com/github/jabrythehutt/awsrd/maintainability",
      "url": "https://api.codeclimate.com/v1/badges/53f17e26baec16b7dd69/maintainability"
    },
    {
      "description": "Build",
      "url": "https://github.com/jabrythehutt/awsrd/actions/workflows/main.yml/badge.svg",
      "href": "https://github.com/jabrythehutt/awsrd/actions?query=branch%3Amain"
    }
  ],
  "activationEvents": [],
  "contributes": {
    "configuration": {
      "title": "EC2",
      "properties": {
        "awsrd.profile": {
          "type": "string",
          "default": "default",
          "description": "AWS profile"
        },
        "awsrd.region": {
          "type": {
            "type": [
              "string",
              "null"
            ],
            "description": "AWS region",
            "default": null,
            "enum": [
              "us-east-2",
              "us-east-1",
              "us-west-1",
              "us-west-2",
              "af-south-1",
              "ap-east-1",
              "ap-south-2",
              "ap-southeast-3",
              "ap-southeast-4",
              "ap-south-1",
              "ap-northeast-3",
              "ap-northeast-2",
              "ap-southeast-1",
              "ap-southeast-2",
              "ap-northeast-1",
              "ca-central-1",
              "eu-central-1",
              "eu-west-1",
              "eu-west-2",
              "eu-south-1",
              "eu-west-3",
              "eu-south-2",
              "eu-north-1",
              "eu-central-2",
              "me-south-1",
              "me-central-1",
              "sa-east-1"
            ]
          }
        }
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "ec2-explorer",
          "title": "AWS Remote Development",
          "icon": "media/instances_dark.svg"
        }
      ]
    },
    "views": {
      "ec2-explorer": [
        {
          "id": "awsrd.instances",
          "name": "Instances",
          "icon": "media/instances_light.svg"
        }
      ]
    },
    "commands": [
      {
        "command": "awsrd.open",
        "title": "Connect in new window",
        "icon": {
          "dark": "media/new_window_dark.svg",
          "light": "media/new_window_light.svg"
        }
      },
      {
        "command": "awsrd.stop",
        "title": "Stop instance",
        "icon": {
          "light": "media/stop_light.svg",
          "dark": "media/stop_dark.svg"
        }
      },
      {
        "command": "awsrd.start",
        "title": "Start instance",
        "icon": {
          "light": "media/play_light.svg",
          "dark": "media/play_dark.svg"
        }
      },
      {
        "command": "awsrd.selectProfile",
        "title": "Select AWS profile",
        "icon": {
          "light": "media/user_light.svg",
          "dark": "media/user_dark.svg"
        }
      },
      {
        "command": "awsrd.selectRegion",
        "title": "Select AWS region",
        "icon": {
          "light": "media/globe_light.svg",
          "dark": "media/globe_dark.svg"
        }
      },
      {
        "command": "awsrd.refresh",
        "title": "Refresh instances list",
        "icon": {
          "light": "media/refresh_light.svg",
          "dark": "media/refresh_dark.svg"
        }
      },
      {
        "command": "awsrd.create",
        "title": "Create a development EC2 instance",
        "icon": {
          "light": "media/add_light.svg",
          "dark": "media/add_dark.svg"
        }
      },
      {
        "command": "awsrd.delete",
        "title": "Delete instance",
        "icon": {
          "light": "media/trash_light.svg",
          "dark": "media/trash_dark.svg"
        }
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "awsrd.selectProfile",
          "when": "view == awsrd.instances",
          "group": "navigation@0"
        },
        {
          "command": "awsrd.selectRegion",
          "when": "view == awsrd.instances",
          "group": "navigation@1"
        },
        {
          "command": "awsrd.refresh",
          "when": "view == awsrd.instances",
          "group": "navigation@2"
        },
        {
          "command": "awsrd.create",
          "when": "view == awsrd.instances",
          "group": "navigation@3"
        }
      ],
      "view/item/context": [
        {
          "command": "awsrd.stop",
          "group": "inline@0",
          "when": "view == awsrd.instances && viewItem =~ /running/"
        },
        {
          "command": "awsrd.start",
          "group": "inline@1",
          "when": "view == awsrd.instances && viewItem =~ /stopped/"
        },
        {
          "command": "awsrd.open",
          "group": "inline@2",
          "when": "view == awsrd.instances && (viewItem =~ /stopped/ || viewItem =~ /running/)"
        },
        {
          "command": "awsrd.delete",
          "group": "inline@3",
          "when": "view == awsrd.instances && (viewItem =~ /stopped/) && viewItem =~ /managed/"
        }
      ]
    }
  }
}
