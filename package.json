{
  "name": "ec2vsc",
  "type": "module",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "synth": "cdk synth",
    "deploy": "cdk deploy",
    "clean": "rm -rf dist",
    "build:ssmPlugin": "cd session-manager-plugin && docker build -t session-manager-plugin-image . && docker run -it --rm --name session-manager-plugin -v `pwd`:/session-manager-plugin session-manager-plugin-image make release",
    "build:ssmPluginDist": "cp -r session-manager-plugin/bin/*_plugin dist/",
    "build:ssmScript": "esbuild ssmProxyScript.ts --bundle --sourcemap --sources-content=true  --platform=node --outdir=dist --out-extension:.js=.cjs",
    "build:keyPair": "ts-node writeSshConfig.ts --privateKeyPath=dist/id_rsa --publicKeyPath=dist/id_rsa.pub --configPath dist/config --proxyScriptPath dist/ssmProxyScript.cjs",
    "deploy:config": "pnpm run build && cp dist/config ~/.ssh/config",
    "build": "tsc --noEmit && pnpm run build:ssmScript && pnpm run build:keyPair"
  },
  "keywords": [],
  "author": "Daniel Jabry <djabry@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "2.69.0-alpha.0",
    "@aws-cdk/aws-redshift-alpha": "2.69.0-alpha.0",
    "@aws-cdk/aws-synthetics-alpha": "2.69.0-alpha.0",
    "@aws-sdk/client-ec2": "^3.297.0",
    "@aws-sdk/client-ssm": "^3.297.0",
    "aws-cdk-lib": "^2.69.0",
    "cdk-monitoring-constructs": "^3.0.3",
    "constructs": "^10.1.284",
    "ssm-session": "^1.0.6",
    "ws": "^8.13.0",
    "yargs": "^17.7.1"
  },
  "devDependencies": {
    "@types/node": "^18.15.5",
    "@types/ws": "^8.5.4",
    "@types/yargs": "^17.0.23",
    "aws-cdk": "^2.69.0",
    "esbuild": "^0.17.12",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  }
}
